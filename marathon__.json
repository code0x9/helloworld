{
    "id": "hello-world",
    "cmd": "",
    "env": {
        "PORT": "8000"
    },
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "idock.daumkakao.io/hello:latest",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 8000
            }]
        }
    },
    "healthChecks": [{
        "protocol": "HTTP",
        "path": "/",
        "gracePeriodSeconds": 3,
        "intervalSeconds": 10,
        "portIndex": 0,
        "timeoutSeconds": 10,
        "maxConsecutiveFailures": 3
    }, {
        "protocol": "TCP",
        "gracePeriodSeconds": 3,
        "intervalSeconds": 5,
        "portIndex": 1,
        "timeoutSeconds": 5,
        "maxConsecutiveFailures": 3
    }, {
        "protocol": "COMMAND",
        "command": {
            "value": "curl -f -X GET http://$HOST:$PORT0/health"
        },
        "maxConsecutiveFailures": 3
    }]
}